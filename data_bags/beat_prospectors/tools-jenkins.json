{
  "id": "tools-jenkins",
  "app_files_to_watch": {
    "jenkins": {
      "document_type" : "jenkins",
      "paths" : ["/var/log/jenkins/jenkins.log"],
      "input_type" : "log",
      "close_inactive": "30m",
      "close_renamed" : true,
      "symlink" : "enabled",
      "tail-files" : true
    },
    "nginx-jenkins": {
      "document_type" : "jenkins-nginx-error",
      "paths" : ["/var/log/ngnix/jenkins.error*"],
      "input_type" : "log",
      "close_inactive": "5m",
      "close_renamed" : true,
      "symlink" : "enabled",
      "tail-files" : true
    }
  },
  "app_procs_to_watch": {
    "nginx_site": {
      "module": "nginx",
      "metricsets":["stubstatus"],
      "ports":["80"]
    }
  },
  "grok_patterns": {

  }

}