{
  "id": "qa-dec",
  "app_files_to_watch": {

  "gateway-service": {
    "document_type": "gateway-service",
      "paths": [
        "/var/log/containers/phenom-gateway-service*/application.log"
      ],
    "input_type": "log",
    "close_inactive": "5m",
    "symlink": "enabled",
    "tail-files": true,
    "multiline.pattern": "^[[:digit:]]{4}-[[:digit:]]{2}-[[:digit:]]{2}",
    "multiline.negate": true,
    "multiline.match": "after"
    },
    "kafka-logs":{
      "document_type": "kafka-logs",
      "paths": [
        "/var/log/kafka/server.log",
        "/var/log/kafka/state-change.log",
        "/var/log/kafka/controller.log",
        "/var/log/kafka/kafka-*.log"
      ],
      "input_type": "log",
      "close_inactive": "5m",
      "symlink": "enabled",
      "tail-files": true,
      "multiline.pattern": "^[[:digit:]]{4}-[[:digit:]]{2}-[[:digit:]]{2}",
      "multiline.negate": true,
      "multiline.match": "after"
    },
    "kafka-gc":{
      "document_type": "kafka-gc",
      "paths": [
        "/var/log/kafka/kafkaServer-gc.log"
      ],
      "input_type": "log",
      "close_inactive": "5m",
      "symlink": "enabled",
      "tail-files": true,
      "multiline.pattern": "^[[::space]]",
      "multiline.negate": false,
      "multiline.match": "after"

    },
    "kafka-events":{
      "document_type": "kafka-events",
      "paths": [
        "/var/log/kafka_logs_backup/*.txt"
      ],
      "input_type": "log",
      "close_inactive": "5m",
      "symlink": "enabled",
      "tail-files": true
    },
     "phenomtrackapi": {
      "document_type": "phenomtrackapi",
      "paths": [
        "/var/log/containers/phenomtrackapi*/phenomtrackapi.log"
      ],
      "input_type": "log",
      "close_inactive": "5m",
      "symlink": "enabled",
      "tail-files": true,
      "multiline.pattern": "^[[:digit:]]{4}-[[:digit:]]{2}-[[:digit:]]{2}",
      "multiline.negate": true,
      "multiline.match": "after"
    },
    "zookeeper":{
      "document_type":"zookeeper",
      "paths": [
        "/var/log/zookeeper/zookeeper.out"
      ],
      "input_type": "log",
      "close_inactive": "5m",
      "symlink": "enabled",
      "tail-files": true
    } 
  },
  "app_procs_to_watch": {

  },
  "grok_patterns": {

  }
}
