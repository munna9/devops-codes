      "emailcampaign": {
        "image_name": "cp_email-campaign",
        "tag_name": "QA-CP",
        "ports": {
          "9058": "9000"
        },
        "environment_variables": [

        ],
        "volumes": {
          "/var/log/containers/emailcampaign": "/opt/deployment/cp-emailcampaign/logs",
          "/opt/phenom/buildproperties/cp-email-campaign/QA1": "/opt/deployment/buildproperties"
