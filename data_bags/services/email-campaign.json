      "emailcampaign": {
        "image_name": "cp_email-campaign",
        "tag_name": "DEV-CP",
        "ports": {
          "9058": "9000"
        },
        "environment_variables": [

        ],
        "volumes": {
          "/var/log/containers/emailcampaign": "/opt/deployment/cp-emailcampaign/logs",
          "/opt/phenom/buildproperties/cp-email-campaign/DEV1": "/opt/deployment/buildproperties"
        }
      }
    },
    "nginx-pod":{
      "uri": "ecompaign-dev.phenompro.com",
