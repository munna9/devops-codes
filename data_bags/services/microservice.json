{
  "id": "microservice",
  "dev-cms": {
    "ecr": {
      "microservice": {
        "image_name": "cp_microservice",
        "tag_name": "DEV-CP",
        "ports": {
          "9014": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf"
        ],
        "volumes": {
          "/var/log/containers/microservice": "/opt/deployment/logs/cp_microservice",
          "/opt/phenom/buildproperties/cp-microservice/DEV": "/opt/deployment/buildproperties"
        }
      }
    }
  }

}