{
  "id": "microservice",
  "dev-candidates": {
    "ecr": {
      "microservice": {
        "image_name": "cp_microservice",
        "tag_name": "DEV-CP",
        "ports": {
          "9057": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf"
        ],
        "volumes": {
          "/var/log/containers/microservice": "/opt/deployment/cp-microservice/logs",
          "/opt/phenom/buildproperties/cp-microservice/DEV": "/opt/deployment/buildproperties"
        }
      }
    }
  },
  "qa-candidates": {
    "ecr": {
      "microservice": {
        "image_name": "cp_microservice",
        "tag_name": "QA-CP",
        "ports": {
          "9057": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf"
        ],
        "volumes": {
          "/var/log/containers/microservice": "/opt/deployment/cp-microservice/logs",
          "/opt/phenom/buildproperties/cp-microservice/QA1": "/opt/deployment/buildproperties"
        }
      }
    }
  },
  "nginx-pod":{
    "uri": "qa2-microservice2.phenompeople.com",
    "nginx_template":"https_template",
    "service_port":443,
    "application_port":9057
  }

}