    "ecr": {
      "emailcampaign": {
        "image_name": "cp_emailcampaign",
        "tag_name": "DEV-CP",
        "ports": {
          "9015": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf"
