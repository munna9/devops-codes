    "ecr": {
      "savejobs-service": {
        "image_name": "ph_save-jobs-service",
        "tag_name": "DEV-CMS",
        "ports": {
          "9005": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=configuration.conf"
