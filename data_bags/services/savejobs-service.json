        "volumes": {
          "/var/log/containers/savejobs-service": "/opt/deployment/logs/ph_save-jobs-service",
          "/opt/phenom/buildproperties/hub-site/ph-save-jobs-service/DEV": "/opt/deployment/buildproperties"
        }
      }
    }
  }

}