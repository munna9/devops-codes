{
  "id": "cp-usermanagement",
  "dev-cms": {
    "ecr": {
      "cp-usermanagement": {
        "image_name": "cp-usermanagement",
        "tag_name": "DEV-CP",
        "ports": {
          "9013": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf"
        ],
        "volumes": {
          "/etc/localtime": "/etc/localtime",
          "/opt/phenom/logs/cp-usermanagement": "/opt/deployment/logs/cp-usermanagement",
          "/opt/phenom/buildproperties/cp-usermanagement/DEV": "/opt/deployment/buildproperties"
        }
      }
    }
  }

}