{
  "id": "facets",
  "dev-candidates": {
    "ecr": {
      "facets": {
        "image_name": "cp_facets",
        "tag_name": "DEV-CP",
        "ports": {
          "9053": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf"
        ],
        "volumes": {
          "/var/log/containers/facets": "/opt/deployment/logs/facets",
          "/opt/phenom/buildproperties/cp-facets/DEV": "/opt/deployment/buildproperties"
        }
      }
    }
  },
  "qa-candidates": {
    "ecr": {
      "facets": {
        "image_name": "cp_facets",
        "tag_name": "QA-CP",
        "ports": {
          "9053": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf"
        ],
        "volumes": {
          "/var/log/containers/facets": "/opt/deployment/logs/facets",
          "/opt/phenom/buildproperties/cp-facets/QA1": "/opt/deployment/buildproperties"
        }
      }
    },
    "nginx-pod":{
      "uri": "qa-facets2.phenompeople.com",
      "nginx_template":"https_template",
      "service_port":443,
      "application_port":9053
    }
  }
}