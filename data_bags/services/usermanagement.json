    "ecr": {
      "usermanagement": {
        "image_name": "cp_usermanagement",
        "tag_name": "DEV-CP",
        "ports": {
          "9054": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf"
