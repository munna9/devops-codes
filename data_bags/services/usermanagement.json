{
  "id": "usermanagement",
  "dev-candidates": {
    "ecr": {
      "usermanagement": {
        "image_name": "cp_usermanagement",
        "tag_name": "DEV-CP",
        "ports": {
          "9054": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf"
        ],
        "volumes": {
          "/var/log/containers/usermanagement": "/opt/deployment/cp_usermanagement/logs",
          "/opt/phenom/buildproperties/accounts/DEV": "/opt/deployment/buildproperties"
        }
      }
    }
  },
  "qa-candidates": {
    "ecr": {
      "usermanagement": {
        "image_name": "cp_usermanagement",
        "tag_name": "QA-CP",
        "ports": {
          "9054": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf"
        ],
        "volumes": {
          "/var/log/containers/usermanagement": "/opt/deployment/cp-usermanagement/logs",
          "/opt/phenom/buildproperties/accounts/QA1": "/opt/deployment/buildproperties"
        }
      }
    },
    "nginx-pod":{
      "uri": "qa-usermanagement2.phenompeople.com",
      "nginx_template":"https_template",
      "service_port":443,
      "application_port":9054
    }
  }
}