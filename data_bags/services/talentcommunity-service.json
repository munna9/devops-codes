{
  "id": "talentcommunity-service",
  "dev-cms": {
    "ecr": {
      "talentcommunity-service": {
        "image_name": "ph_talent-community",
        "tag_name": "DEV-CMS",
        "ports": {
          "9009": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=configuration.conf"
        ],
        "volumes": {
          "/opt/phenom/logs/talentcommunity-service": "/opt/deployment/logs/ph_talent-community",
          "/opt/phenom/buildproperties/hub-site/ph-talent-community-alerts/DEV": "/opt/deployment/buildproperties"
        }
      }
    }
  }

}