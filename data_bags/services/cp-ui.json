{
  "id": "cp-ui",
  "dev-candidates": {
    "ecr": {
      "cp-ui": {
        "image_name": "cu_appui_2",
        "tag_name": "DEV-CP",
        "ports": {
          "9050": "80"
        },
        "environment_variables": [
          "DIRECTORY_ROOT=cpui/dev-docker"

        ],
        "hostname":true,        
        "volumes": {
          "/var/log/containers/cp-ui": "/var/log/nginx"
        }
      }
    },
    "nginx-pod":{
      "uri": "dev-cpui.phenompro.com",
      "nginx_template":"http_template",
      "service_port":80,
      "application_port":9050
    }
  },
