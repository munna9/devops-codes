{
  "id": "candidates",
  "qa-candidates": {
    "ecr": {
      "candidates": {
        "image_name": "cp_candidates",
        "tag_name": "QA-CP",
        "ports": {
          "9062": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf",
          "JAVA_OPTS=-Xmx1g"
        ],
        "volumes": {
          "/var/log/containers/candidates": "/opt/deployment/candidates/logs",
          "/opt/phenom/buildproperties/candidates/QA1": "/opt/deployment/buildproperties",
          "/opt/phenom/CsvFiles": "/opt/deployment/CsvFiles",
          "/opt/phenom/ExcelFiles": "/opt/deployment/ExcelFiles",
          "/opt/phenom/EmailAutoUpload":"/opt/deployment/EmailAutoUpload",
          "/opt/phenom/images":"/opt/deployment/images"
        }
      }
    },
    "nginx-pod":{
      "uri": "candidatesapi2.phenompeople.com",
      "nginx_template":"https_template",
      "service_port":443,
      "application_port":9062
    }
  }
}