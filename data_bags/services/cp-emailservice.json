{
  "id": "cp-email-microservice",
  "dev-cms": {
    "ecr": {
      "cp-email-microservice": {
        "image_name": "cp-email-microservice",
        "tag_name": "DEV-CP",
        "ports": {
          "9012": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf"
        ],
        "volumes": {
          "/etc/localtime": "/etc/localtime",
          "/opt/phenom/logs/cp-email-microservice": "/opt/deployment/logs/cp-email-microservice",
          "/opt/phenom/buildproperties/cp-email-microservices/DEV": "/opt/deployment/buildproperties"
        }
      }
    }
  }

}