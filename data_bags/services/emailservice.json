{
  "id": "emailservice",
  "dev-candidates": {
    "ecr": {
      "emailservice": {
        "image_name": "cp_email-microservice",
        "tag_name": "DEV-CP",
        "ports": {
          "9056": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf"
        ],
        "volumes": {
          "/var/log/containers/emailservice": "/opt/deployment/cp-email-microservice/logs",
          "/opt/phenom/buildproperties/cp-email-microservices/DEV": "/opt/deployment/buildproperties"
        }
      }
    }
  },
  "qa-candidates": {
    "ecr": {
      "emailservice": {
        "image_name": "cp_email-microservice",
        "tag_name": "QA-CP",
        "ports": {
          "9056": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=application.conf"
        ],
        "volumes": {
          "/var/log/containers/emailservice": "/opt/deployment/cp-email-microservice/logs",
          "/opt/phenom/buildproperties/cp-email-microservices/QA1": "/opt/deployment/buildproperties"
        }
      }
    },
    "nginx-pod":{
      "uri": "qa-emailservice2.aws.phenom.local",
      "nginx_template":"http_template",
      "service_port":80,
      "application_port":9056
    }
  }


}