    "ecr": {
      "emailservice": {
        "image_name": "cp_email-microservice",
        "tag_name": "DEV-CP",
        "ports": {
          "9056": "9000"
        },
        "environment_variables": [

        ],
        "hostname":true,
        "volumes": {
          "/var/log/containers/emailservice": "/opt/deployment/cp-email-microservice/logs",
          "/opt/phenom/candidates-buildproperties/emailservice": "/opt/deployment/buildproperties"
        }
      }
    },
    "nginx-pod":{
      "uri": "dev-emailservice.aws.phenom.local",
      "nginx_template":"http_template",
      "service_port":80,
      "application_port":9056
