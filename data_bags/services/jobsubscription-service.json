{
  "id": "jobsubscription-service",
  "dev-cms": {
    "ecr": {
      "jobsubscription-service": {
        "image_name": "ph_job-subscription",
        "tag_name": "DEV-CMS",
        "ports": {
          "9032": "9000"
        },
        "environment_variables": [
          "CONF_FILE_NAME=configuration.conf"
        ],
        "volumes": {
          "/var/log/containers/jobsubscription-service": "/opt/deployment/logs/ph_job-subscription",
          "/opt/phenom/buildproperties/hub-site/ph-job-subscription-service/DEV": "/opt/deployment/buildproperties"
        }
      }
    }
  }

}