{
  "id":"kibana",
  "tools":{
    "docker":{
      "kibana":{
        "image_name":"phenompeople/kibana",
        "tag_name":"latest",
        "ports":{
          "5601":"5601"
        },
        "environment_variables":[
          "ELASTICSEARCH_URL=http://dev-elasticsearch01.aws.phenom.local",
          "KIBANA_INDEX=.kibana",
          "SERVER_HOST=0.0.0.0"
        ],
        "volumes":{
          "/etc/localtime":"/etc/localtime",
          "/opt/phenom/logs/kibana": "/opt/kibana/logs"
        }
      }
    }
  },
  "prod-cpapp":{
    "docker":{
      "kibana":{
        "image_name":"phenompeople/kibana",
        "tag_name":"5.3.0",
        "ports":{
          "5601":"5601"
        },
        "environment_variables":[
          "ELASTICSEARCH_URL=http://172.25.1.129:9200",
          "KIBANA_INDEX=.kibana",
          "SERVER_HOST=0.0.0.0"
        ],
        "volumes":{
          "/etc/localtime":"/etc/localtime",
          "/opt/phenom/logs/kibana": "/opt/kibana/logs"
        }
      }
    }
  }

}