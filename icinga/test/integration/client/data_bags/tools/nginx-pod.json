{
  "id":"nginx-pod",
  "_default":{
    "docker":{
      "nginx-pod":
      {
        "image_name": "phenompeople/nginx-lua",
        "tag_name": "pod",
        "ports": {
          "80"  : "80",
          "443" : "443"
        },
        "environment_variables": [
        ],
        "volumes": {
          "/etc/nginx/conf.d": "/etc/nginx/conf.d",
          "/var/log/nginx":"/var/log/nginx"
        }
      }
    }
  }
}