  "name": "qa-analytics",
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "description": "QA environment specfication file for candidates",

  "cookbook_versions": {

  },

  "default_attributes": {
    "aws_account_id":"716533421362",
    "aws_region":"us-east-1",
    "cloud":{
