{
  "name": "dev-candidates",
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "description": "Dev environment specfication file for candidates",

  "cookbook_versions": {

  },

  "default_attributes": {
    "aws_account_id":"716533421362",
    "aws_region":"us-east-1",
    "chef-client":{
      "pool":{
        "interval": 300
      }
    },
    "mongodb":{
      "replication_name":"rs0",
      "key_authentication":true,
      "major":{
        "version": "3.2"
      }
    },
    "es_cluster_name"   : "DEV_CP_CONTAINER",
    "elasticsearch": {
      "server_port": 9200,
      "elb_hostname": "dev-elasticsearch-cand.aws.phenom.local",
      "elb_port"    : 9200

    }
  },

  "override_attributes": {
    "build-essentials":{
      "buildproperties_repo":{
        "uri":"git@bitbucket.org:maheimom/candidates-buildproperties.git",
        "checkout_directory":"candidates-buildproperties",
        "branch":"DEV"
      }
    },
    "elasticsearch":{
      "package":{
        "version": "2.4.5"
      },
      "nginx":{
        "shield": true
      }
    }

  }
}
