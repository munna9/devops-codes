{
  "name": "qa",
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "description": "QA environment specfication file",

  "cookbook_versions": {
  },

  "default_attributes": {
    "elk_cluster_name"  : "elkcluster"
  },

  "override_attributes": {
    "elasticsearch":{
      "server_name": "qa-elasticsearch.aws.phenom.local",
      "server_port": "9200",
      "package":{
        "version": "5.4.1-1"
      }
    },
    "logstash": {
      "server_name": "qa-logstash01.aws.phenom.local",
      "server_port": "5004",
      "certificate_name": "qacert"
    }
  }
}
