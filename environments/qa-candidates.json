{
  "name": "qa-candidates",
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "description": "QA environment specfication file for candidates",

  "cookbook_versions": {
    "mongodb":"0.1.9"
  },

  "default_attributes": {
    "aws_account_id":"716533421362",
    "aws_region":"us-east-1",
    "chef-client":{
      "pool":{
        "interval": 300
      }
    },
    "candidates":{
      "elb_hostname": "qa-candidatesapi2.aws.phenom.local"
    },
    "usermanagement":{
      "lb_hostname": "qa-usermanagement2.aws.phenom.local"
    },
    "mongodb":{
      "replication_name":"crs1",
      "key_authentication":true,
      "major":{
        "version": "3.2"
      }
    },
    "mysql_tag_name":"qa-mysql-node",
    "logstash": {
      "server_name": "qa-logstash01.aws.phenom.local",
      "server_port": "5004",
      "certificate_name": "qacert"
    },
    "elasticsearch": {
      "elb_hostname": "qa-elasticsearch-elb.aws.phenom.local",
      "elb_port"    : 9200
    },
    "mysql":{
      "elb_hostname": "qa-mysql.aws.phenom.local"
    },
    "es_cluster_name"   : "QA_CP_CONTAINER",
    "nginx":{
      "addon-lb":{
        "candidates":{
          "uri": "qa-candidatesapi2.aws.phenom.local",
          "nginx_template":"http_template",
          "service_port":80
        },
        "usermanagement" : {
          "uri": "qa-usermanagement2.aws.phenom.local",
          "nginx_template":"http_template",
          "service_port":80
        },
        "login-auth-server" : {
          "uri": "qa-auth-server.phenompeople.com",
          "nginx_template":"http_tomcat_template",
          "context_name":"osiam-auth-server",
          "service_port":80
        },
        "login-resource-server" :{
          "uri": "qa-resource-server.phenompeople.com",
          "nginx_template":"http_tomcat_template",
          "context_name":"osiam-resource-server",
          "service_port":80
        },
        "login-self-administration" :{
          "uri": "qa-self-administration.phenompeople.com",
          "nginx_template":"http_tomcat_template",
          "context_name":"addon-administration",
          "service_port":80
        }
      }
    }
  },

  "override_attributes": {
    "elasticsearch":{
      "package":{
        "version": "2.3.5"
      }
    }
  
  }
}

