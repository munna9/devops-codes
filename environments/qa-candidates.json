{
  "name": "qa-candidates",
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "description": "QA environment specfication file for candidates",

  "cookbook_versions": {
    "aws":"0.1.3",
    "basic-essentials":"0.1.14",
    "build-essentials" :"0.1.10",
    "candidate-jobs":"0.1.0",
    "candidates":"0.1.5",
    "chef-client":"0.1.4",
    "cp-ui":"0.1.0",
    "data-ui":"0.1.0",
    "data-api":"0.1.0",
    "docker": "0.1.12",
    "email-campaign": "0.1.0",
    "emailservice": "0.1.0",
    "elasticsearch":"0.1.13",
    "facets":"0.1.0",
    "filebeat":"0.1.2",
    "metricbeat" :"0.1.4",
    "microservice":"0.1.2",
    "mongodb":"0.1.9",
    "ntp":"0.1.7",
    "nginx":"0.1.11",
    "login-self-administration":"0.1.0",
    "login-resource-server":"0.1.0",
    "okta-auth":"0.1.0",
    "sendgridwebhook":"0.1.0",
    "unsubscribe": "0.1.0",
    "usermanagement":"0.1.3",
    "usermanage-ui":"0.1.0"
  },

  "default_attributes": {
    "aws_account_id":"716533421362",
    "aws_region":"us-east-1",
    "chef-client":{
      "pool":{
        "interval": 300
      }
    },
    "mongodb":{
      "replication_name":"crs1",
      "key_authentication":true,
      "major":{
        "version": "3.2"
      }
    },
    "logstash": {
      "server_name": "qa-logstash01.aws.phenom.local",
      "server_port": "5004",
      "certificate_name": "qacert"
    },
    "elasticsearch": {
      "server_name" : "qa-elasticsearch.aws.phenom.local",
      "server_port" : 9200,
      "elb_hostname": "qa-elasticsearch-elb.aws.phenom.local",
      "elb_port"    : 9200
    },
    "mysql":{
      "elb_hostname": "qa-mysql.aws.phenom.local"
    },
    "es_cluster_name"   : "QA_CP_CONTAINER"
  },
  "override_attributes": {
    "elasticsearch":{
      "package":{
        "version": "2.3.5"
      },
      "nginx": {
        "shield": true
      }
    }
  }
}

